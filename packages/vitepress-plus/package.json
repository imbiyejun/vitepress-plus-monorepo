{
  "name": "@imbiyejun/vitepress-plus",
  "version": "0.1.0",
  "type": "module",
  "description": "Enhanced VitePress template with topic management and article status",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./template": "./template"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest"
  },
  "keywords": [
    "vitepress",
    "documentation",
    "template"
  ],
  "peerDependencies": {
    "vitepress": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitepress": "^1.6.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "vue": "^3.4.0"
  }
}

